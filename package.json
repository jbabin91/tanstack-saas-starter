{
  "name": "tanstack-saas-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm with-env vinxi build",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "db:check": "pnpm with-env drizzle-kit check --config drizzle.config.ts",
    "db:drop": "pnpm with-env drizzle-kit drop --config drizzle.config.ts",
    "db:generate": "pnpm with-env drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "pnpm with-env drizzle-kit push --config drizzle.config.ts",
    "db:reset": "pnpm with-env tsx src/db/reset.ts",
    "db:seed": "pnpm with-env tsx src/db/seed.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config drizzle.config.ts",
    "dev": "pnpm with-env vinxi dev",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "prepare": "NODE_OPTIONS='--experimental-transform-types --disable-warning ExperimentalWarning' husky",
    "serve": "pnpm with-env vite preview",
    "start": "pnpm with-env vinxi start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:storybook": "test-storybook --maxWorkers=1 --ci",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenvx run --"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.39.1",
