    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.12",
    "storybook-dark-mode": "^4.0.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.2",
    "vitest": "^3.1.1"
  },
  "packageManager": "pnpm@10.8.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
