{
  "name": "tanstack-saas-starter",
  "private": true,
  "type": "module",
  "scripts": {
    "auth:generate": "pnpm with-env pnpm dlx @better-auth/cli@latest generate --config src/lib/auth.ts --output src/db/schema/auth.ts",
    "build": "pnpm with-env vinxi build",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "db:check": "pnpm with-env drizzle-kit check --config drizzle.config.ts",
    "db:down": "docker compose -f docker-compose.yml down",
    "db:drop": "pnpm with-env drizzle-kit drop --config drizzle.config.ts",
    "db:generate": "pnpm with-env drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate --config drizzle.config.ts",
    "db:push": "pnpm with-env drizzle-kit push --config drizzle.config.ts",
    "db:reset": "pnpm with-env tsx src/db/reset.ts",
    "db:seed": "pnpm with-env tsx src/db/seed.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config drizzle.config.ts",
    "db:up": "docker compose -f docker-compose.yml up -d",
    "dev": "pnpm with-env vinxi dev",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "i18n:gen": "tsx scripts/generate-translations.ts",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/eslint/.eslintcache",
    "prepare": "husky",
    "start": "pnpm with-env vinxi start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:storybook": "test-storybook --maxWorkers=1 --ci",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenvx run --"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.2",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-form": "^1.12.4",
    "@tanstack/react-query": "^5.75.4",
    "@tanstack/react-query-devtools": "^5.75.4",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "@tanstack/react-router-with-query": "^1.119.0",
    "@tanstack/react-start": "^1.119.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "better-auth": "^1.2.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "country-state-city": "^3.2.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "input-otp": "^1.4.2",
    "libphonenumber-js": "^1.12.25",
    "lucide-react": "^0.507.0",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "postcode-validator": "^3.10.5",
    "radix-ui": "^1.4.3",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.5",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.5.8",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.76",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.50.0",
    "@eslint/js": "^9.26.0",
    "@faker-js/faker": "^9.8.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-themes": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.1",
    "@storybook/theming": "^8.6.12",
    "@tailwindcss/postcss": "^4.1.5",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitest/browser": "^3.1.3",
    "@vitest/coverage-v8": "^3.1.3",
    "axe-playwright": "^2.1.0",
    "chromatic": "^11.29.0",
    "commitlint": "^19.8.1",
    "cz-git": "^1.11.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import-x": "^4.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "i18n-iso-countries": "^7.14.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "playwright": "^1.52.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "storybook": "^8.6.12",
    "storybook-dark-mode": "^4.0.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  }
}
