    "@types/react-dom": "^19.1.2",
    "@vitest/browser": "3.1.1",
    "@vitest/coverage-v8": "3.1.1",
    "axe-playwright": "^2.1.0",
    "chromatic": "^11.28.2",
    "commitlint": "^19.8.0",
    "cz-git": "^1.11.1",
    "drizzle-kit": "^0.31.0",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import-x": "^4.10.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
