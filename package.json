    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.4.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "vinxi": "^0.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.3",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
